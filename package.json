{
  "name": "starwar-challenge",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "axios": "^0.16.2",
    "classnames": "^2.2.5",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "qs": "^6.5.1",
    "ramda": "^0.24.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-scripts": "1.0.13",
    "react-window-resize-listener": "^1.1.0"
  },
  "scripts": {
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/stylesheets -o src/stylesheets --watch --recursive",
    "start-cra": "react-scripts start",
    "start": "npm-run-all -p watch-css start-cra",
    "docs": "cp -a ./build/. ./docs/",
    "build": "npm run build-css && react-scripts build && npm run docs",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
